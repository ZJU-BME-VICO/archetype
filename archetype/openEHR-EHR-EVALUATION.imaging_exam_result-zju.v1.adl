definition
    EVALUATION[at0000] matches {	--	Imaging exam result
        subject matches {
            PARTY_SELF[at0010] matches {	--	Patient
                externalRef matches {
                    PARTY_REF[at0011] matches {	--	Patient
                        id matches {
                            GENERIC_ID matches {*}
                        }
                    }
                }
            }
        }
        data matches {
            ITEM_TREE[at0001] matches {	--	structure
                items cardinality matches {1..*; ordered} matches {
                    ELEMENT[at0002] matches {	--	Exam description
                        value matches {
                            DV_TEXT matches {*}
                        }
                    }
                    ELEMENT[at0003] occurrences matches {0..1} matches {	--	DateTime result issued
                        value matches {
                            DV_DATE_TIME matches {*}
                        }
                    }
                    ELEMENT[at0004] occurrences matches {0..1} matches {	--	Findings
                        value matches {
                            DV_TEXT matches {*}
                        }
                    }
                    ELEMENT[at0005] occurrences matches {0..1} matches {	--	Conclusion
                        value matches {
                            DV_TEXT matches {*}
                        }
                    }
                    ELEMENT[at0006] occurrences matches {0..1} matches {	--	Memo
                        value matches {
                            DV_TEXT matches {*}
                        }
                    }
                    ELEMENT[at0007] occurrences matches {0..1} matches {	--	Result representation
                        value matches {
                            DV_MULTIMEDIA matches {*}
                        }
                    }
                }
            }
        }
        provider matches {
            PARTY_SELF[at0008] matches {	--	Reporter
                externalRef matches {
                    PARTY_REF[at0009] matches {	--	Reporter
                        id matches {
                            GENERIC_ID matches {*}
                        }
                    }
                }
            }
        }
        otherParticipations cardinality matches {0..*; unordered} matches {
            PARTICIPATION[at0012] occurrences matches {0..1} matches {	--	Reviewer
                performer matches {
                    PARTY_RELATED matches {
                        externalRef matches {
                            PARTY_REF matches {
                                id matches {
                                    GENERIC_ID matches {*}
                                }
                            }
                        }
                        relationship matches {
                            DV_CODED_TEXT matches {
                                defining_code matches {
                                    [local::at0012]
                                }
                            }
                        }
                    }
                }
            }
        }
    }

